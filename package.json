{
  "name": "react-cc-generator",
	"displayName": "react-cc-generator",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:react-cc-generator.genReactComponentContainerFiles"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
			"command": "react-cc-generator.genReactComponentContainerFiles",
			"title": "New React Component"

		}],
    "menus": {
      "explorer/context": [
        {
          "command": "react-cc-generator.genReactComponentContainerFiles",
          "group": "navigation"
        }
      ]
    }
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.59.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "eslint": "^7.27.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2"
    }

}
